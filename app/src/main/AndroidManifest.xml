<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brook.app.android.dependentlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.brook.app.android.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.brook.app.android.AboveToActivity" />
        <activity android:name="com.brook.app.android.AlignActivity" />
        <activity android:name="com.brook.app.android.AlignParentActivity" />
        <activity android:name="com.brook.app.android.MarginAndPaddingActivity" />
        <activity android:name="com.brook.app.android.ToCenterActivity" />
        <activity android:name="com.brook.app.android.ToParentCenterActivity" />
        <activity android:name="com.brook.app.android.SimpleActivity"></activity>
    </application>

</manifest>